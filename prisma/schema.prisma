// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id  String @id
  pet String
  Pet Pet[]
}

model Pet {
  name    String @id
  owner   User   @relation(fields: [userId], references: [id])
  ownerID String
}
